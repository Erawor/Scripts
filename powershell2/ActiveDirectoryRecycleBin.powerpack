          <IconB64></IconB64>
          <script><![CDATA[ #region Make sure that the AD forest is in Windows 2008 R2 or later mode.
$forest = Get-ADForest
if ($forest.ForestMode -ne [Microsoft.ActiveDirectory.Management.ADForestMode]::Windows2008R2Forest) {
	if ([System.Windows.Forms.MessageBox]::Show("The Active Directory Recycle Bin feature only works in forests running in Windows Server 2008 R2 mode. The $($forest.Name) forest is currently in $(switch ($forest.ForestMode) { 'Windows2000Forest' {'Windows 2000 Server'; break} 'Windows2003InterimForest' {'Windows Server 2003 Interim'; break} 'Windows2003Forest' {'Windows Server 2003'; break} 'Windows2008Forest' {'Windows Server 2008'; break} default {'an unknown'}}) forest mode.$([System.Environment]::NewLine * 2)Would you like to upgrade the forest mode for $($forest.Name) to Windows Server 2008 R2 mode?",'Forest mode upgrade required',[System.Windows.Forms.MessageBoxButtons]::YesNo,[System.Windows.Forms.MessageBoxIcon]::Question,[System.Windows.Forms.MessageBoxDefaultButton]::Button2) -eq [System.Windows.Forms.DialogResult]::Yes) {
		$forest | Set-ADForestMode -ForestMode Windows2008R2Forest -Confirm:$false
	} else {
		return
	}
}
#endregion

#region Make sure that the Recycle Bin feature is enabled.
$partitionsContainer = Get-ADObject $forest.PartitionsContainer -Properties 'msDS-EnabledFeature'
$recycleBinFeature = Get-ADOptionalFeature -Identity '766ddcd8-acd0-445e-f3b9-a7f9b6744f2a'
if ($partitionsContainer.'msDS-EnabledFeature' -notcontains $recycleBinFeature.DistinguishedName) {
	if ([System.Windows.Forms.MessageBox]::Show("The Active Directory Recycle Bin feature is currently disabled in $($forest.Name). In order to use this PowerPack, you must first enable this feature.$([System.Environment]::NewLine * 2)WARNING: This action cannot be undone.$([System.Environment]::NewLine * 2)Would you like to enable the Active Directory Recycle Bin in $($forest.Name)?",'Feature not enabled',[System.Windows.Forms.MessageBoxButtons]::YesNo,[System.Windows.Forms.MessageBoxIcon]::Question,[System.Windows.Forms.MessageBoxDefaultButton]::Button2) -eq [System.Windows.Forms.DialogResult]::Yes) {
		Enable-ADOptionalFeature -Identity '766ddcd8-acd0-445e-f3b9-a7f9b6744f2a' -Scope ForestOrConfigurationSet -Target $forest.Name -Confirm:$false -WarningAction SilentlyContinue
	} else {
		return
	}
}
#endregion

#region Retrieve the contents of the recycle bin.
$rootDSE = Get-ADRootDSE
$recycleBin = Get-ADObject -SearchBase $rootDSE.defaultNamingContext -SearchScope OneLevel -IncludeDeletedObjects -LdapFilter '(name=Deleted Objects)'
$global:containerClasses = ('container','organizationalUnit','msExchAdminGroup','msExchExchangeServer','msExchRecipientPolicyContainer','msExchSystemPolicyContainer','protocolCfgSharedServer','sitesContainer','crossRefContainer','physicalLocation','lostAndFound','mSMQEnterpriseSettings','site','interSiteTransportContainer','subnetContainer','builtinDomain','domainDNS','lostAndFound','infrastructureUpdate','computer','server','crossRef','serversContainer','licensingSiteSettings','nTDSSiteSettings','nTFRSSubscriptions','nTFRSSettings','dfsConfiguration','domainPolicy','classStore','msExchTransportSettings')
Get-ADDeletedObject -RecycleBinDistinguishedName $recycleBin.DistinguishedName
#endregion]]></script>
