function global:Restore-ADRecycleBinObjectRecursive {
	[CmdletBinding()]
	param(
		[Parameter(Mandatory=$true,
		           ValueFromPipeline=$true)]
		[Microsoft.ActiveDirectory.Management.ADObject]
		$Identity,

		[Parameter(Mandatory=$false)]
		[string]
		$TargetPath
	)

	begin {
		$rootDSE = Get-ADRootDSE
		$recycleBin = Get-ADObject -SearchBase $rootDSE.defaultNamingContext -SearchScope OneLevel -IncludeDeletedObjects -LdapFilter '(name=Deleted Objects)'
	}
	process {
		$recursiveProcess = {
			param(
				[Microsoft.ActiveDirectory.Management.ADObject] $Identity,
				[string]                                        $TargetPath
			)
			
			$recycleBinObject = Get-ADObject -Identity $Identity -IncludeDeletedObjects -Properties 'lastKnownParent'
			$recycleBinChildObjects = @(Get-ADObject -SearchBase $recycleBin.DistinguishedName -IncludeDeletedObjects -Properties 'lastKnownParent' -Filter * | Where-Object {$_.lastKnownParent -eq $recycleBinObject.DistinguishedName})
			$recycleBinObject = Restore-ADObject -Identity $recycleBinObject.ObjectGUID -TargetPath $(if ($TargetPath) {$TargetPath} else {$recycleBinObject.lastKnownParent}) -PassThru
			if ($recycleBinChildObjects.Count) {
				foreach ($item in $recycleBinChildObjects) {
					& $recursiveProcess -Identity $item.ObjectGUID -TargetPath $recycleBinObject.DistinguishedName
				}
			}
		}
		& $recursiveProcess -Identity $Identity -TargetPath $TargetPath
	}
}

function global:Remove-ADRecycleBinObjectRecursive {
	[CmdletBinding(SupportsShouldProcess=$true,
	               ConfirmImpact='Low')]
	param(
		[Parameter(Mandatory=$true,
		           ValueFromPipeline=$true)]
		[Microsoft.ActiveDirectory.Management.ADObject]
		$Identity
	)

	begin {
		$rootDSE = Get-ADRootDSE
		$recycleBin = Get-ADObject -SearchBase $rootDSE.defaultNamingContext -SearchScope OneLevel -IncludeDeletedObjects -LdapFilter '(name=Deleted Objects)'
	}
	process {
		$recursiveProcess = {
			param(
				[Microsoft.ActiveDirectory.Management.ADObject] $Identity
			)

			$recycleBinObject = Get-ADObject -Identity $Identity -IncludeDeletedObjects -Properties 'lastKnownParent'
			$recycleBinChildObjects = @(Get-ADObject -SearchBase $recycleBin.DistinguishedName -IncludeDeletedObjects -Properties 'lastKnownParent' -Filter * | Where-Object {$_.lastKnownParent -eq $recycleBinObject.DistinguishedName})
			if ($PSCmdlet.ShouldProcess($recycleBinObject.DistinguishedName,'Remove-ADObject')) {
				Remove-ADObject -Identity $recycleBinObject.ObjectGUID -IncludeDeletedObjects -Confirm:$false
				if ($recycleBinChildObjects.Count) {
					foreach ($item in $recycleBinChildObjects) {
						& $recursiveProcess -Identity $item.ObjectGUID
					}
				}
			}
		}
		& $recursiveProcess -Identity $Identity
	}
}

function global:Get-ADDeletedObject {
	param(
		[string] $ParentDistinguishedName     = $null,
		[string] $RecycleBinDistinguishedName = $null
	)
	#region Define the filter to get the list of objects one level deep.
	if ($ParentDistinguishedName) {
		$filter = '*'
		$whereClause = {$_.lastKnownParent -eq $ParentDistinguishedName}
	} else {
		$filter = 'distinguishedName -ne $RecycleBinDistinguishedName'
		$whereClause = {$_.lastKnownParent -notmatch "$RecycleBinDistinguishedName`$"}
	}
	#endregion

	#region Retrieve the contents of the container.
	$recycleBinObjectProperties = @(
		@{
			Name = 'Name'
			Expression = {$_.'msDS-LastKnownRDN'}
		},
		@{
			Name = 'OriginalLocation'
			Expression = {$_.lastKnownParent}
		},
		@{
			Name = 'IsContainer'
			Expression = {$global:containerClasses -contains $_.ObjectClass}
		},
		@{
			Name = 'WhenDeleted'
			Expression = {
				$xml = [xml]"<root>$(([string]($_.'msDS-ReplAttributeMetaData')).Replace([char]0,' '))</root>"
				foreach ($item in ($xml.root.DS_REPL_ATTR_META_DATA | Where-Object {$_.pszAttributeName -eq 'isDeleted'})) {
					[System.DateTime]::Parse($item.ftimeLastOriginatingChange)
					break
				}
			}
		},
		@{
			Name = 'WhereDeleted'
			Expression = {
				$xml = [xml]"<root>$(([string]($_.'msDS-ReplAttributeMetaData')).Replace([char]0,' '))</root>"
				foreach ($item in ($xml.root.DS_REPL_ATTR_META_DATA | Where-Object {$_.pszAttributeName -eq 'isDeleted'})) {
					if ($dcWhereDeleted = Get-ADObject -Identity ($item.pszLastOriginatingDsaDN -replace '^CN\=NTDS Settings\,','') -Properties DNSHostName -ErrorAction SilentlyContinue) {
						if ($dcWhereDeleted.DNSHostName) {
							$dcWhereDeleted.DNSHostName
						} elseif ($dcWhereDeleted.Name) {
							$dcWhereDeleted.Name
						} else {
							$item.uuidLastOriginatingDsaInvocationID
						}
					} else {
						$item.uuidLastOriginatingDsaInvocationID
					}
					break
				}
			}
		},
		'ObjectClass',
		'ObjectGUID',
		'DistinguishedName',
		@{
			Name = 'ADObject'
			Expression = {$_}
		}
	)
	$defaultDisplayPropertySet = New-Object System.Management.Automation.PSPropertySet('DefaultDisplayPropertySet',[string[]]@('Name','OriginalLocation','IsContainer','WhenDeleted','WhereDeleted'))
	$PSStandardMembers = [System.Management.Automation.PSMemberInfo[]]@($defaultDisplayPropertySet)
	$deletedObjectList = @()
	$processRecycleBinObject = {
		$recycleBinObject = $_
		$recycleBinObject.PSObject.TypeNames.Clear()
		if (-not $ParentDistinguishedName) {
			$recycleBinObject.PSObject.TypeNames.Add('Microsoft.ActiveDirectory.Management.RecycleBin.ADObject')
		} else {
			$recycleBinObject.PSObject.TypeNames.Add('Microsoft.ActiveDirectory.Management.RecycleBin.ADObject#Leaf')
		}
		switch ($recycleBinObject.ObjectClass) {
			'user' {
				$recycleBinObject.PSObject.TypeNames.Insert(0,'Microsoft.ActiveDirectory.Management.RecycleBin.ADUser')
				break
			}
			'group' {
				$recycleBinObject.PSObject.TypeNames.Insert(0,'Microsoft.ActiveDirectory.Management.RecycleBin.ADGroup')
				break
			}
			'msExchDynamicDistributionList' {
				$recycleBinObject.PSObject.TypeNames.Insert(0,'Microsoft.ActiveDirectory.Management.RecycleBin.ADGroup')
				break
			}
			'computer' {
				$recycleBinObject.PSObject.TypeNames.Insert(0,'Microsoft.ActiveDirectory.Management.RecycleBin.ADComputer')
				break
			}
			default {
				if ($recycleBinObject.IsContainer) {
					$recycleBinObject.PSObject.TypeNames.Insert(0,'Microsoft.ActiveDirectory.Management.RecycleBin.ADContainer')
					Add-AdminConsoleRecycleBinContainerObject -DeletedObject $recycleBinObject -RecycleBinDistinguishedName $RecycleBinDistinguishedName
				} else {
					$recycleBinObject.PSObject.TypeNames.Insert(0,"Microsoft.ActiveDirectory.Management.RecycleBin.ADObject#$($recycleBinObject.ObjectClass)")
				}
				break
			}
		}
		$recycleBinObject
	}
	Get-ADObject -SearchBase $RecycleBinDistinguishedName -IncludeDeletedObjects -Filter $filter -Properties 'msDS-ReplAttributeMetaData','msDS-LastKnownRDN','lastKnownParent' `
		| ForEach-Object {
			$deletedObjectList += $_.DistinguishedName
			$_
		} `
		| Where-Object $whereClause `
		| Select-Object -Property $recycleBinObjectProperties `
		| Add-Member -Force -Name PSStandardMembers -MemberType MemberSet -Value $PSStandardMembers -PassThru `
		| Add-Member -Force -MemberType ScriptMethod -Name ToString -Value {$this.DistinguishedName} -PassThru `
		| ForEach-Object $processRecycleBinObject
	if (-not $ParentDistinguishedName) {
		Get-ADObject -SearchBase $RecycleBinDistinguishedName -IncludeDeletedObjects -Filter $filter -Properties 'msDS-ReplAttributeMetaData','msDS-LastKnownRDN','lastKnownParent' `
			| Where-Object {($_.lastKnownParent -match "$RecycleBinDistinguishedName`$") -and ($deletedObjectList -notcontains $_.lastKnownParent)} `
			| Select-Object -Property $recycleBinObjectProperties `
			| Add-Member -Force -Name PSStandardMembers -MemberType MemberSet -Value $PSStandardMembers -PassThru `
			| Add-Member -Force -MemberType ScriptMethod -Name ToString -Value {$this.DistinguishedName} -PassThru `
			| ForEach-Object $processRecycleBinObject
	}
	#endregion
}

function global:Add-AdminConsoleRecycleBinContainerObject {
	param(
		[PSObject] $DeletedObject,
		[string]   $RecycleBinDistinguishedName
	)

	$selectedNode = $Host.PrivateData.ConsoleHostFactory.Application.Navigation.CurrentItem

	$newNodeName = $DeletedObject.Name

	$newNodeScript = {
	param(
		[string] $DeletedObjectDistinguishedName = $null,
		[string] $RecycleBinDistinguishedName    = $null
	)

	Get-ADDeletedObject -ParentDistinguishedName $DeletedObjectDistinguishedName -RecycleBinDistinguishedName $RecycleBinDistinguishedName
}

	Add-AdminConsoleDynamicScriptNode -ParentNode $selectedNode -Name $newNodeName -Script $newNodeScript -ScriptParameters @($DeletedObject.DistinguishedName, $RecycleBinDistinguishedName) -IconTypeIdentifier 'Microsoft.ActiveDirectory.Management.RecycleBin.ADContainer'
}

#endregion]]></script>
        </container>
      </items>
    </container>
  </items>
</configuration>