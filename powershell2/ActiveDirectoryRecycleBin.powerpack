            <container id="78997940-659d-46fc-a1b5-302fe62cd17d" name="Modify..." Description="" type="Action@" RequireSelection="No" DisplayResults="Refresh">
              <IconB64></IconB64>
              <script><![CDATA[$fieldDescriptions = @(
	(New-FieldDescription -Name 'DeletedObjectLifetime' -HelpMessage 'The number of days before an object in the recycle bin is tombstoned' -Type 'System.Int32' -DefaultValue $global:recycleBinConfiguration.DeletedObjectLifetime),
	(New-FieldDescription -Name 'TombstoneLifetime' -HelpMessage 'The number of days before a tombstoned object is permanently deleted' -Type 'System.Int32' -DefaultValue $global:recycleBinConfiguration.TombstoneLifetime)
)
$options = Read-Input -Caption 'Modify Properties' -Message "Use the fields below to modify the properties of the Active Directory recycle bin configuration.$([System.Environment]::NewLine * 2)WARNING: Microsoft recommends using 180 days for each of these values. Changing these to less than 180 days may result in your recycle bin objects being tombstoned earlier than you anticipated." -FieldDescription $fieldDescriptions
if ($options.Count) {
	$update = $false
	if ($options['DeletedObjectLifetime'] -and ($global:recycleBinConfiguration.DeletedObjectLifetime -ne $options['DeletedObjectLifetime'])) {
		$global:recycleBinConfiguration.DeletedObjectLifetime = $options['DeletedObjectLifetime']
		$update = $true
	}
	if ($options['TombstoneLifetime'] -and ($global:recycleBinConfiguration.TombstoneLifetime -ne $options['TombstoneLifetime'])) {
		$global:recycleBinConfiguration.TombstoneLifetime = $options['TombstoneLifetime']
		$update = $true
	}
	if ($update) {
		$rootDSE = Get-ADRootDSE
		Set-ADObject -Identity "CN=Directory Service,CN=Windows NT,CN=Services,$($rootDSE.ConfigurationNamingContext)" -Partition $rootDSE.ConfigurationNamingContext -Replace @{'msDS-DeletedObjectLifetime'=[System.Int32]$global:recycleBinConfiguration.DeletedObjectLifetime;'tombstoneLifetime'=[System.Int32]$global:recycleBinConfiguration.TombstoneLifetime}
	}
}]]></script>
              <Types>
                <type>ActiveDirectory.RecycleBin.Configuration.Property</type>
              </Types>
