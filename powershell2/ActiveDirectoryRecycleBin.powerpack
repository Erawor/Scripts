            <container id="6d3631b1-2f17-42ba-ba17-6366023e9a6b" name="Configure recycle bin" Description="" type="Action@" returntype="ActiveDirectory.RecycleBin.Configuration.Property" RequireSelection="No" DisplayResults="NestedView">
              <IconB64></IconB64>
              <script><![CDATA[#region Initialize the configuration object.
$global:recycleBinConfiguration = New-Object System.Management.Automation.PSObject `
	| Add-Member -MemberType NoteProperty -Name DeletedObjectLifetime -Value 180 -PassThru `
	| Add-Member -MemberType NoteProperty -Name TombstoneLifetime -Value 180 -PassThru
$global:recycleBinConfiguration.PSObject.TypeNames.Clear()
$global:recycleBinConfiguration.PSObject.TypeNames.Add('ActiveDirectory.RecycleBin.Configuration')
#endregion

#region Read the current values from AD and set them in the configuration object.
$rootDSE = Get-ADRootDSE
$directoryService = Get-ADObject -Identity "CN=Directory Service,CN=Windows NT,CN=Services,$($rootDSE.ConfigurationNamingContext)" -Partition $rootDSE.ConfigurationNamingContext -Properties 'msDS-DeletedObjectLifetime','tombstoneLifetime'
if ($directoryService.'msDS-DeletedObjectLifetime') {
	$global:recycleBinConfiguration.DeletedObjectLifetime = $directoryService.'msDS-DeletedObjectLifetime'
}
if ($directoryService.tombstoneLifetime) {
	$global:recycleBinConfiguration.TombstoneLifetime = $directoryService.tombstoneLifetime
}
#endregion

#region Write out the configuration object in list format.
foreach ($property in $global:recycleBinConfiguration.PSObject.Properties | Select-Object -ExpandProperty Name) {
	$configurationProperty = New-Object PSObject
	$configurationProperty.PSObject.TypeNames.Clear()
	$configurationProperty.PSObject.TypeNames.Insert(0,'ActiveDirectory.RecycleBin.Configuration.Property')
	$configurationProperty `
		| Add-Member -MemberType NoteProperty -Name Name -Value $property -PassThru `
		| Add-Member -MemberType NoteProperty -Name Value -Value $(if ($global:recycleBinConfiguration.$property.Count) {"'$([System.String]::Join(''', ''',$global:recycleBinConfiguration.$property))'"} else {$global:recycleBinConfiguration.$property}) -PassThru
}
#endregion]]></script>
