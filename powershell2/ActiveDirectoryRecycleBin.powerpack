            <container id="001ddc5c-decf-44e3-9db2-6ae76bd2e114" name="Empty recycle bin" Description="" type="Action@" RequireSelection="No" DisplayResults="Refresh">
              <IconB64></IconB64>
              <script><![CDATA[if ([System.Windows.Forms.DialogResult]::Yes -eq [System.Windows.Forms.MessageBox]::Show("You are about to empty your Active Directory recycle bin.  This action cannot be undone.$([System.Environment]::NewLine * 2)Are you sure you want to permanently delete the contents of your Active Directory recycle bin?",'Confirm operation',[System.Windows.Forms.MessageBoxButtons]::YesNo,[System.Windows.Forms.MessageBoxIcon]::Warning,[System.Windows.Forms.MessageBoxDefaultButton]::Button2)) {
	$rootDSE = Get-ADRootDSE
	$recycleBin = Get-ADObject -SearchBase $rootDSE.defaultNamingContext -SearchScope OneLevel -IncludeDeletedObjects -LdapFilter '(name=Deleted Objects)'
	Get-ADObject -SearchBase $recycleBin.DistinguishedName -IncludeDeletedObjects -Filter 'distinguishedName -ne $recycleBin.DistinguishedName' `
		| Remove-ADObject -IncludeDeletedObjects -Confirm:$false
}]]></script>
